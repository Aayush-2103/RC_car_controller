{
  "expo": {
    "name": "AutoCart",
    "slug": "AutoCart",
    "version": "1.1.0",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "autocart",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#18cdd6",
        "foregroundImage": "./assets/images/autocart-android-icon-foreground.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.AutoCart"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/autocart-android-icon-foreground.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#18cdd6",
          "dark": {
            "backgroundColor": "#18cdd6"
          }
        }
      ],
      [
        "react-native-permissions",
        {
          "iosPermissions": [
            "NSBluetoothAlwaysUsageDescription"
          ]
        }
      ]

    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
